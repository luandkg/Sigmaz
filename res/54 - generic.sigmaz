struct Item in ( T ) {
	restrict : 
	define mGuardar : T = null;
	define mNivel : T;
	all : 
	act guardar ( eGuardar : T ) {
		mGuardar = eGuardar;
	}

	act nivelar ( eNivel : T ) {
		mNivel = eNivel;
	}
	func passar ( ) : T {
		return mGuardar;
	}

	act cortar ( a : int ) {
		reg @ R5 -> a;
		PROC -> {
			SET R13;
			MOV "";
			INT_STRING R5;
			PRINT R13;
		}
		
	}
	
}

