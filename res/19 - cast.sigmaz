operation == ( a : pos , b : pos ) : bool {
	def c : bool;
	def c1 : num = num ( a );
	def c2 : num = num ( b );
	invoke math -> operator_equal ( c1 , c2 ) :: c;
	return c;
}

operation !! ( a : pos , b : pos ) : bool {
	def c : bool;
	def c1 : num = a;
	def c2 : num = b;
	invoke math -> operator_not ( c1 , c2 ) :: c;
	return c;
}

