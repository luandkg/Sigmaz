call iniciar -> inicio ( );

act inicio ( ) {
	def valor : num = 1;
	def ternal : num = -> ( maior ( valor , 2 ) ) :: ( +1 ) not ( -1 );
	invoke __COMPILER__ -> SHOW_SCOPE ( ) :: ALL;
}
func maior ( a : num , b : num ) : bool {
	def c : bool;
	invoke math -> operator_great ( a , b ) :: c;
	return c;
}
