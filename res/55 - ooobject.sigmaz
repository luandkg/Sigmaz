call iniciar -> pular ( );

struct Bloco {

    all:
        define Proximo : Caixa = null;

        act setValor(a:num) {valor = a;}
     restrict:
        define valor : num = 0;

}

struct Caixa {

    all:
        act gravar(a:num) {valor = a;}
     restrict:
        define valor : num = 0;

}

act pular ( ) {

    def P1 : Bloco = init Bloco();
    P1.setValor(2);

    def P2 : Caixa = init Caixa();
    P2.gravar(5);

    P1.Proximo = P2;

    P1.Proximo.gravar(4);


}
