func int ( a : num ) : int {
	def b : int;
	invoke math -> operator_num_to_int ( a ) :: b;
	return b;
}
func num ( a : int ) : num {
	def b : num;
	invoke math -> operator_int_to_num ( a ) :: b;
	return b;
}
operator ++ ( a : int , b : num ) : num {
	def c : num;
	def x : num = num ( a );
