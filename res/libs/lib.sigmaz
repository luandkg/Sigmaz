type Tipado {
	define A : int = 0;
	define B : int = 0;
}
operator ++ ( a : Tipado , b : Tipado ) : Tipado {
	return start Tipado {
		A = somar ( a.A , b.A );
		B = somar ( a.B , b.B );
	};
	
}

