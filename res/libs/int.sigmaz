

# OPERADORES INT
func int ( a : int ) : int {
	return a;
}


operator ++ ( a : int , b : int ) : int {
	reg @R5 -> a;
    reg @R6 -> b;

     PROC -> {

        SET R7;
            MOV 0;
            OPE R5 ADD R6;

     }

	return reg @R7;
}
