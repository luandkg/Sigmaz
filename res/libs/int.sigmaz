func menor_igual ( a : int , b : int ) : bool {

    def ret : bool = false;

	reg @R5 -> a;
    reg @R6 -> b;

     PROC -> {

        SET R1;
            MOV FALSE;
            OPE R5 LESS R6;

     }

	def e_menor : bool =  reg @R1;

		reg @R5 -> a;
        reg @R6 -> b;

         PROC -> {

            SET R1;
                MOV FALSE;
                OPE R5 EQUAL R6;

         }

    	def e_igual : bool =  reg @R1;



	if (e_menor) -> { ret = true; }
	if (e_igual) -> { ret = true; }


	return ret;

}
