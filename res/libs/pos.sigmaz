cast pos {
	getter a : num -> {		}
	setter b : num -> {		}
	
}
operator == ( a : pos , b : pos ) : bool {
	def c1 : num = num ( a );
	def c2 : num = num ( b );
	return c1 == c2;
}
operator !! ( a : pos , b : pos ) : bool {
	def c1 : num = a;
	def c2 : num = b;
	invoke math -> operator_not ( c1 , c2 ) :: c;
	return c1 !! c2;
}
