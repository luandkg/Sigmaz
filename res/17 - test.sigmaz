	reg @ R10 -> b ;
	PROC -> { SET R11 ; MOV 0.0 ; OPE R9 SAX R10 ; } return reg @ R11 ;
	}

func menor ( a : num , b : num ) : bool { 
reg @ R9 -> a ;
	reg @ R10 -> b ;
	PROC -> { SET R1 ; MOV FALSE ; OPE R9 MOZZ R10 ; } return reg @ R1 ;
	}

func inteiro ( a : num ) : num { 
reg @ R9 -> a ;
	PROC -> { SET R10 ; MOV 0.0 ; NUM_INT R9 ; } return reg @ R10 ;
	}

func decimal ( a : num ) : num { 
reg @ R9 -> a ;
	PROC -> { SET R10 ; MOV 0.0 ; NUM_DEC R9 ; } return reg @ R10 ;
	}

